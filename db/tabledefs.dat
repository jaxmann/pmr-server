CREATE TABLE Player
(
FullName varchar(60),
FirstName varchar(30),
LastName varchar(30),
CommonName varchar(30),
TeamName varchar(50),
LeagueName varchar(50)
);

CREATE TABLE Team
(
TeamName varchar(50),
LeagueName varchar(50)
);

CREATE TABLE User
(
Username varchar(30),
Email varchar(50),
PasswordHash varchar(80),
PasswordSalt varchar(80),
PhoneNumber varchar(10),
Keywords varchar(100000),
UNIQUE(Username, Email, PhoneNumber) ON CONFLICT ABORT
);
